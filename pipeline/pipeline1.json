{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "BinarySource",
						"storeSettings": {
							"type": "HttpReadSettings",
							"requestMethod": "GET"
						},
						"formatSettings": {
							"type": "BinaryReadSettings"
						}
					},
					"sink": {
						"type": "BinarySink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "Git_Binary",
						"type": "DatasetReference",
						"parameters": {
							"baseURL": {
								"value": "@pipeline().parameters.sourceBaseURL",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "ADLS_Binary",
						"type": "DatasetReference",
						"parameters": {
							"baseURL": {
								"value": "@pipeline().parameters.targetBaseURL",
								"type": "Expression"
							},
							"container": {
								"value": "@pipeline().parameters.container",
								"type": "Expression"
							},
							"directory": {
								"value": "@pipeline().parameters.directory",
								"type": "Expression"
							},
							"fileName": {
								"value": "@pipeline().parameters.fileName",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"sourceBaseURL": {
				"type": "string",
				"defaultValue": "https://github.com/abhishek-s-k/Data_Ingestion_ADLS/tree/main/Data/Dental_Insurance_Dataset/"
			},
			"targetBaseURL": {
				"type": "string",
				"defaultValue": "https://globalstorageaccount.dfs.core.windows.net"
			},
			"container": {
				"type": "string",
				"defaultValue": "git-healthinsurance"
			},
			"directory": {
				"type": "string",
				"defaultValue": "individualInsurance"
			},
			"fileName": {
				"type": "string",
				"defaultValue": "Individual_Market_Dental2021.xlsx"
			}
		},
		"annotations": []
	}
}
{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ExcelSource",
						"storeSettings": {
							"type": "HttpReadSettings",
							"requestMethod": "GET"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "Git_Excel",
						"type": "DatasetReference",
						"parameters": {
							"baseURL": {
								"value": "@pipeline().parameters.sourceBaseURL",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"sourceBaseURL": {
				"type": "string",
				"defaultValue": "https://github.com/abhishek-s-k/Data_Ingestion_ADLS/tree/main/Data/Dental_Insurance_Dataset"
			}
		},
		"annotations": []
	}
}
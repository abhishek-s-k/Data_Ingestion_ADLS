{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ExcelSource",
						"storeSettings": {
							"type": "HttpReadSettings",
							"requestMethod": "GET"
						}
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "ParquetWriteSettings"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "Git_Excel",
						"type": "DatasetReference",
						"parameters": {
							"baseURL": {
								"value": "@pipeline().parameters.sourceBaseURL",
								"type": "Expression"
							},
							"sheetName": "Individual_Market_Dental"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "ADLS_Parquet",
						"type": "DatasetReference"
					}
				]
			}
		],
		"parameters": {
			"sourceBaseURL": {
				"type": "string",
				"defaultValue": "https://github.com/abhishek-s-k/Data_Ingestion_ADLS/blob/main/Data/Dental_Insurance_Dataset/Individual_Market_Dental2021.xlsx"
			},
			"targetBaseURL": {
				"type": "string",
				"defaultValue": "https://globalstorageaccount.dfs.core.windows.net"
			},
			"container": {
				"type": "string",
				"defaultValue": "git-healthinsurance"
			},
			"directory": {
				"type": "string",
				"defaultValue": "individualInsurance"
			},
			"fileName": {
				"type": "string",
				"defaultValue": "Individual_Market_Dental2021.xlsx"
			}
		},
		"annotations": []
	}
}